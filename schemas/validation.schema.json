{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "coverage_score",
    "unsupported_claims",
    "missing_topics",
    "issues"
  ],
  "properties": {
    "coverage_score": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "unsupported_claims": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "missing_topics": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "issues": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}